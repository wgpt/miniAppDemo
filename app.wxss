/*重置图片大小*/
image{
    width: 200rpx;
    height: 200rpx;
}

/*居中*/
img{
    max-width: 100%;
    max-height: 100%;
    display: flex;
    justify-content: center;
    align-items: center;
}

/*水平上下居中*/
.vw{
    display: flex;
    white-space:nowrap;
    align-items: center;
    flex-direction: row;
}


/*垂直水平居中*/
.vh{
    display: flex;
    justify-content: center;
    flex-direction: column;
}